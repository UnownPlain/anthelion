{
	"$schema": "https://github.com/UnownPlain/anthelion/raw/main/src/schema/task/schema.json",
	"packageId": "HomeBank.HomeBank",
	"strategy": "page-match",
	"pageMatch": {
		"url": "https://www.gethomebank.org/en/downloads.php",
		"regex": "href=\"[^\"]*HomeBank[._-](\\d+(?:\\.\\d+)+)[._-]setup\\.exe\""
	},
	"urls": ["https://www.gethomebank.org/public/binaries/HomeBank-{version}-setup.exe"]
}
