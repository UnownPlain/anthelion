{
	"$schema": "https://anthelion.unownplain.dev/schema.json",
	"packageId": "Google.CloudSDK",
	"strategy": "page-match",
	"pageMatch": {
		"url": "https://docs.cloud.google.com/sdk/docs/install-sdk",
		"regex": "Install\\s+gcloud\\s+CLI\\s+version\\s+(\\d+(?:\\.\\d+)+)"
	},
	"urls": ["https://dl.google.com/dl/cloudsdk/channels/rapid/GoogleCloudSDKInstaller.exe"],
	"replace": true
}
