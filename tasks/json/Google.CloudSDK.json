{
	"$schema": "https://github.com/UnownPlain/anthelion/raw/main/src/schema/task/schema.json",
	"packageId": "Google.CloudSDK",
	"strategy": "page-match",
	"pageMatch": {
		"url": "https://cloud.google.com/sdk/docs/install-sdk",
		"regex": "latest\\s*gcloud\\s*CLI\\s*version\\s*\\(v?(\\d+(?:\\.\\d+)+)\\)"
	},
	"urls": [
		"https://dl.google.com/dl/cloudsdk/channels/rapid/GoogleCloudSDKInstaller.exe"
	],
	"replace": true
}
