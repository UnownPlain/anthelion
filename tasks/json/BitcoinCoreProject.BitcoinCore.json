{
	"$schema": "https://github.com/UnownPlain/anthelion/raw/main/src/schema/task/schema.json",
	"packageId": "BitcoinCoreProject.BitcoinCore",
	"strategy": "page-match",
	"pageMatch": {
		"url": "https://bitcoincore.org/en/download/",
		"regex": "href=.*?bitcoin[._-]core[._-](\\d+(?:\\.\\d+)+)/bitcoin[._-](\\d+(?:\\.\\d+)+)[._-]win64[._-]setup\\.exe"
	},
	"urls": [
		"https://bitcoincore.org/bin/bitcoin-core-{version}/bitcoin-{version}-win64-setup.exe"
	]
}
