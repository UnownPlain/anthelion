{
	"$schema": "https://anthelion.unownplain.dev/schema.json",
	"strategy": "page-match",
	"pageMatch": {
		"url": "https://nginx.org/",
		"regex": "nginx[._-]v?(\\d+(?:\\.\\d+)+)<\\/a>[\\s\\S]*?mainline version"
	},
	"urls": ["https://nginx.org/download/nginx-{version}.zip|x64"],
	"releaseNotesUrl": "https://nginx.org/en/CHANGES"
}
