{
	"$schema": "https://anthelion.unownplain.dev/schema.json",
	"packageId": "Oracle.MySQLWorkbench",
	"strategy": "page-match",
	"pageMatch": {
		"url": "https://dev.mysql.com/downloads/workbench/",
		"regex": "mysql-workbench-community-v?(\\d+(?:\\.\\d+)+)-winx64"
	},
	"urls": [
		"https://cdn.mysql.com/Downloads/MySQLGUITools/mysql-workbench-community-{version}-winx64.msi"
	]
}
