{
	"$schema": "https://anthelion.unownplain.dev/schema.json",
	"packageId": "Flywheel.Local",
	"strategy": "redirect-match",
	"redirectMatch": {
		"url": "https://cdn.localwp.com/stable/latest/windows",
		"regex": "local[._-]v?(\\d+(?:\\.\\d+)+)[._-]windows\\.exe"
	},
	"releaseNotesUrl": "https://localwp.com/releases/{version}"
}
