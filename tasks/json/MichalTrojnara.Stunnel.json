{
	"$schema": "https://github.com/UnownPlain/anthelion/raw/main/src/schema/task/schema.json",
	"packageId": "MichalTrojnara.Stunnel",
	"strategy": "page-match",
	"pageMatch": {
		"url": "https://www.stunnel.org/downloads.html",
		"regex": "href=.*?stunnel[._-]v?(\\d+(?:\\.\\d+)+)\\.t"
	},
	"urls": [
		"https://www.stunnel.org/downloads/stunnel-{version}-win64-installer.exe"
	]
}
