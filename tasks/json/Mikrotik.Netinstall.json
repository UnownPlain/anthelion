{
	"$schema": "https://anthelion.unownplain.dev/schema.json",
	"packageId": "Mikrotik.Netinstall",
	"strategy": "page-match",
	"pageMatch": {
		"url": "https://cdn.mikrotik.com/routeros/latest-stable.rss",
		"regex": "<title>RouterOS\\s+v?(\\d+(?:\\.\\d+)+)"
	},
	"urls": [
		"https://download.mikrotik.com/routeros/{version}/netinstall-{version}.zip",
		"https://download.mikrotik.com/routeros/{version}/netinstall64-{version}.zip"
	]
}
