{
	"$schema": "https://github.com/UnownPlain/anthelion/raw/main/src/schema/task/schema.json",
	"packageId": "Valve.SteamLink",
	"strategy": "redirect-match",
	"redirectMatch": {
		"url": "https://community.chocolatey.org/api/v2/package/steamlink/",
		"regex": "steamlink[._-]v?(\\d+(?:\\.\\d+)+)\\.nupkg"
	},
	"urls": [
		"https://media.steampowered.com/steamlink/windows/SteamLink-{version}.zip"
	]
}
