{
	"$schema": "https://github.com/UnownPlain/anthelion/raw/main/src/schema/task/schema.json",
	"packageId": "XrevealTeam.Xreveal",
	"strategy": "page-match",
	"pageMatch": {
		"url": "https://www.xreveal.com/download.html",
		"regex": "xreveal[._-]v?(\\d+(?:\\.\\d+)+)[._-]x\\d+\\.exe"
	},
	"urls": [
		"https://www.xreveal.com/download/xreveal_{version}_x64.exe",
		"https://www.xreveal.com/download/xreveal_{version}_x86.exe",
		"https://www.xreveal.com/download/xreveal_{version}_arm64.exe"
	]
}
